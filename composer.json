{
    "name": "funnydevjsc/cryptomus-laravel-integrate",
    "description": "Laravel package for cryptomus.com",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "FunnyDev\\Cryptomus\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Funny dev., Jsc",
            "email": "contact@funnydev.vn",
            "homepage": "https://github.com/funnydevjsc"
        }
    ],
    "homepage": "https://github.com/funnydevjsc/cryptomus-laravel-integrate",
    "keywords": ["Laravel", "package", "payment", "cryptomus", "9pay", "sepay", "crypto", "bitcoin", "btc", "litecoin", "ltc", "usdt", "bsc"],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.8",
        "cryptomus/api-php-sdk": "^1.0"
    },
    "require-dev": {
        "illuminate/support": "^8.0",
        "illuminate/http": "^8.83",
        "illuminate/routing": "^8.83"
    },
    "extra": {
        "laravel": {
            "providers": [
                "FunnyDev\\Cryptomus\\CryptomusServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

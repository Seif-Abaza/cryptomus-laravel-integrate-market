{
    "name": "cryptopayment/cryptomus-laravel-integrate-market",
    "description": "Laravel package for cryptomus.com",
    "type": "library",
    "license": "MIT",
    "version": "1.0.6",
    "autoload": {
        "psr-4": {
            "cryptopayment\\Cryptomus\\": "src/"
        }
    },
    "authors": [{
        "name": "Seif Abaza",
        "email": "seif.abaza@yandex.com",
        "homepage": "https://github.com/Seif-Abaza"
    }],
    "homepage": "https://github.com/Seif-Abaza/cryptomus-laravel-integrate-market",
    "keywords": ["Laravel", "package", "payment", "cryptomus", "9pay", "sepay", "crypto", "bitcoin", "btc", "litecoin", "ltc", "usdt", "bsc"],
    "require": {
        "php": "^8.1",
        "cryptomus/api-php-sdk": "^1.0"
    },
    "require-dev": {
        "illuminate/support": "^8.0",
        "illuminate/http": "^8.83",
        "illuminate/routing": "^8.83"
    },
    "extra": {
        "laravel": {
            "providers": [
                "cryptopayment\\Cryptomus\\CryptomusServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}